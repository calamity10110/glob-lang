imp embedded.gpio
imp embedded.wifi
imp embedded.time

asy blink_led(pin, interval_ms):
    loop:
        gpio.high(pin)
        await time.sleep(interval_ms)
        gpio.low(pin)
        await time.sleep(interval_ms)

asy connect_wifi():
    await wifi.connect("YOUR_SSID", "YOUR_PASSWORD")
    print("Connected! IP:", wifi.ip())

mn main():
    gpio.set_mode(2, OUTPUT)
    await connect_wifi()
    await blink_led(2, 1000)
